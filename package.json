{
  "scripts": {
    "dev": "npm run db:setup:dev && ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "npm run db:setup && node dist/src/index.js",
    "db:setup": "npm run migrate && npm run seed",
    "db:setup:dev": "npm run migrate:dev && npm run seed:dev",
    "migrate": "typeorm migration:run -d typeorm.config.js",
    "migrate:dev": "typeorm-ts-node-commonjs migration:run -d typeorm.config.ts",
    "seed": "node dist/src/repositories/seeds/runSeeds.js",
    "seed:dev": "ts-node src/repositories/seeds/runSeeds.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d typeorm.config.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d typeorm.config.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d typeorm.config.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "axios": "^1.13.1",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "graphql": "^16.11.0",
    "ioredis": "^5.8.2",
    "jose": "^6.1.0",
    "pg": "^8.16.3",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@semantic-release-plus/docker": "^3.1.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/connect-redis": "^0.0.23",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.9.1",
    "semantic-release": "^24.2.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
